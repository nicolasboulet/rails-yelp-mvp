<%= simple_form_for [:admin, @restaurant] do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.input :category, collection: ['chinese', 'italian', 'japanese',
                                        'french', 'belgian'] %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
